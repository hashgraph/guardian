{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "cli": {
        "analytics": "68a952b6-061b-4e33-a09f-12cc9a4d3085"
    },
    "newProjectRoot": "projects",
    "projects": {
        "guardian": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:application",
                    "configurations": {
                        "development": {
                          "extractLicenses": false,
                            "namedChunks": true,
                            "optimization": false,
                            "sourceMap": true
                        },
                        "production": {
                            "budgets": [
                                {
                                    "maximumError": "60mb",
                                    "maximumWarning": "15mb",
                                    "type": "initial"
                                },
                                {
                                    "maximumError": "120kb",
                                    "maximumWarning": "30kb",
                                    "type": "anyComponentStyle"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "demo": {
                            "budgets": [
                                {
                                    "maximumError": "60mb",
                                    "maximumWarning": "15mb",
                                    "type": "initial"
                                },
                                {
                                    "maximumError": "120kb",
                                    "maximumWarning": "30kb",
                                    "type": "anyComponentStyle"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.demo.ts"
                                }
                            ],
                            "outputHashing": "all"
                        }
                    },
                    "defaultConfiguration": "development",
                    "options": {
                        "allowedCommonJsDependencies": [
                            "interfaces",
                            "codemirror/addon/edit/closebrackets",
                            "codemirror/addon/edit/matchbrackets",
                            "codemirror/addon/fold/brace-fold",
                            "codemirror/addon/fold/foldcode",
                            "codemirror/addon/fold/foldgutter",
                            "codemirror/addon/fold/indent-fold",
                            "codemirror/lib/codemirror",
                            "codemirror/mode/javascript/javascript",
                            "codemirror/mode/markdown/markdown",
                            "codemirror/mode/yaml/yaml",
                            "codemirror/addon/mode/overlay",
                            "codemirror/addon/mode/simple",
                            "events",
                            "qrcode",
                            "cronstrue",
                            "ajv",
                            "ajv-formats/dist/formats",
                            "file-saver",
                            "moment",
                            "bessel",
                            "jstat",
                            "typed-function",
                            "seedrandom",
                            "javascript-natural-sort",
                            "escape-latex",
                            "codemirror/addon/hint/css-hint",
                            "codemirror/addon/hint/show-hint"
                        ],
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "index": "src/index.html",
                        "inlineStyleLanguage": "scss",
                        "outputPath": {
                          "base": "dist/guardian"
                        },
                        "polyfills": [
                          "src/polyfills.ts"
                        ],
                        "scripts": [
                            "./node_modules/leader-line/leader-line.min.js"
                        ],
                        "styles": [
                          "./node_modules/ngx-toastr/toastr.css",
                          "./node_modules/primeng/resources/themes/saga-blue/theme.css",
                          "./node_modules/primeng/resources/primeng.min.css",
                          "./node_modules/primeicons/primeicons.css",
                          "node_modules/ol/ol.css",
                          "src/styles.scss"
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "browser": "src/main.ts",
                        "preserveSymlinks": true
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "guardian:build"
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "development": {
                            "buildTarget": "guardian:build:development",
                            "proxyConfig": "proxy.conf.json"
                        },
                        "production": {
                            "buildTarget": "guardian:build:production",
                            "proxyConfig": "proxy.conf.json"
                        },
                        "demo": {
                            "buildTarget": "guardian:build:demo",
                            "proxyConfig": "proxy.conf.json"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "inlineStyleLanguage": "scss",
                        "karmaConfig": "karma.conf.js",
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "scripts": [
                            "./node_modules/leader-line/leader-line.min.js"
                        ],
                        "styles": [
                            "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
                            "src/styles.scss"
                        ],
                        "tsConfig": "tsconfig.spec.json"
                    }
                }
            },
            "prefix": "app",
            "projectType": "application",
            "root": "",
            "schematics": {
                "@schematics/angular:application": {
                    "strict": true
                },
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "sourceRoot": "src"
        }
    },
    "version": 1
}
