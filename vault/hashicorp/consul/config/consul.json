{
  "server": true,
  "node_name": "consul",
  "datacenter": "dc1",
  "client_addr": "0.0.0.0",
  "bind_addr": "0.0.0.0",
  "bootstrap_expect": 1,
  "data_dir": "/consul/data",
  "log_level": "DEBUG",
  "ui_config": {
    "enabled": false
  },
  "verify_incoming": true,
  "verify_outgoing": true,
  "verify_server_hostname": true,
  "ca_file": "/consul/tls/ca.crt",
  "cert_file": "/consul/tls/tls.crt",
  "key_file": "/consul/tls/tls.key",
  "auto_encrypt": {
    "allow_tls": true
  },
  "ports": {
    "dns": 53
  }
}
