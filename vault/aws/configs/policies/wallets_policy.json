{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "secretsmanager:CreateSecret",
                "secretsmanager:ListSecrets",
                "secretsmanager:GetSecretValue",
                "secretsmanager:DeleteSecret",
                "secretsmanager:PutSecretValue"
              ],
            "Resource": "arn:aws:secretsmanager:eu-central-1:{{AWS_ACCOUNT_ID}}:secret:*/guardian/wallets/*"
        }
    ]
}
